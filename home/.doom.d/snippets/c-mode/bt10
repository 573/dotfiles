
# -*- mode: snippet -*-
# name: poormans backtrace (depth: 10)
# key: bt10
# --

#define r(depth) __builtin_return_address(depth)
int printf(const char* f,...); printf("%s() at %s:%d\n %p %p %p %p %p %p %p %p %p %p\n", __func__, __FILE__, __LINE__, r(0), r(1), r(2), r(3), r(4), r(5), r(6), r(7), r(8), r(9));
#undef r
  