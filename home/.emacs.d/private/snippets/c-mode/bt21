#define r(depth) __builtin_return_address(depth)
int printf(const char* f,...); printf("%s() at %s:%d\n %p %p %p %p %p %p %p %p %p %p %p %p %p %p %p %p %p %p %p %p %p\n", __func__, __FILE__, __LINE__, r(0), r(1), r(2), r(3), r(4), r(5), r(6), r(7), r(8), r(9), r(10), r(11), r(12), r(13), r(14), r(15), r(16), r(17), r(18), r(19), r(20));
#undef r
  